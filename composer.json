{
    "name": "skrz/bunny-bundle",
    "description": "Symfony bundle that makes using RabbitMQ easy.",
    "keywords": [
        "rabbitmq",
        "rabbit",
        "bunny",
        "amqp",
        "messaging",
        "message",
        "queue",
        "queueing",
        "exchange",
        "symfony",
        "symfonyfw",
        "bundle",
        "symfony bundle"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Jakub Kulhan",
            "email": "jakub.kulhan@gmail.com"
        },
        {
            "name": "Lukas Senfeld",
            "email": "tech@senfeld.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "bunny/bunny": "~0.5",
        "symfony/console": "~3.4 | ~4.4",
        "symfony/framework-bundle": "~3.4 | ~4.4",
        "symfony/serializer": "~3.4 | ~4.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Skrz\\Bundle\\BunnyBundle\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "sort-packages": true,
        "platform": {
            "php": "7.4.22"
        }
    }
}
